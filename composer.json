{
    "name": "nickdekruijk/admin",
    "description": "A simple, lightweight yet complete Laravel 5.5+ admin panel and backend",
    "license": "MIT",
    "keywords": [ "cms", "laravel", "admin", "panel", "backend" ],
    "homepage": "http://www.nickdekruijk.nl/packages/admin",
    "authors": [
        {
            "name": "Nick de Kruijk",
            "email": "git@nickdekruijk.nl"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "^5.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NickDeKruijk\\Admin\\ServiceProvider"
            ],
            "aliases": {
                "LaraPages": "NickDeKruijk\\Admin\\Facade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "NickDeKruijk\\Admin\\": "src/"
        }
    }
}
